{
	"name":         "svg-patterns",
	"description":  "SVG patterns for Data Visualization.",
	"version":      "0.1.0",
	"main":         "index.js",
	"files":        ["index.js", "helpers.js", "p/*.js"],
	"keywords":     ["svg", "dataviz", "pattern", "generate"],
	"author":       "Jannis R <mail@jannisr.de>",
	"homepage":     "https://github.com/derhuerst/svg-patterns",
	"repository":   "derhuerst/svg-patterns",
	"issues":       "https://github.com/derhuerst/svg-patterns/issues",
	"license":      "ISC",
	"engines":      {"node": ">=6"},
	"dependencies": {
		"virtual-dom": "^2.1.1",
		"virtual-dom-stringify": "^3.0.1"
	},
	"devDependencies": {
		"browserify": "^13.1",
		"babelify": "^7.3",
		"babel-preset-es2015": "^6.14",
		"uglify-js": "mishoo/UglifyJS2#harmony"
	},
	"scripts": {
		"bundle": "browserify -t [ babelify --presets es2015 ] demo.js > bundle.js",
		"minify": "uglifyjs -mc --screw-ie8 -- bundle.js > bundle.min.js",
		"build": "npm run bundle && npm run minify",
		"dev": "browserify demo.js > bundle.min.js"
	}
}
